							&--image
								margin 0 auto
								width 100%
								display inline-block
								img 
									width 50px
									margin 0 auto
							&--title
								margin 1em
								h4
									color gris_E
									font-size 1.3em
							&--description
								display none
								p
									color gris_C
									font-weight lighter
							&--icons
								display flex
								justify-content space-around
								flex-wrap wrap
								.Icon__content
									&--icon
										color gris_C
										font-size 1.5em
									&--text
										color gris_D
										display none
