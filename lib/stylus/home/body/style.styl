.Landing
	Container(70px)
	background-color white
	&__container
		.LandingPrimary
			padding-bottom 3em
			display flex
			flex-wrap wrap
			justify-content space-between
			&__left
				display none
				&--image
					background-image url('../images/left.png')
					width 100%
					background-size cover
					background-position right center
					background-repeat no-repeat
					height 550px
			&__center
				padding .5em 0
				text-align center
				width 100%
				&--box
					max-width 400px
					margin 0 auto
					.LandingLogo
						&__image
							width 150px
					.LandingText
						&__description
							color gris_D
							font-weight lighter
							font-size 18px
							span
								color gris_E
					.LandingBtn
						margin .5em 0
						&__action
							border-top 1px solid Verde_Ascent
							border-right 1px solid Verde_Ascent
							border-bottom 3px solid Verde_Ascent
							border-left 1px solid Verde_Ascent
							margin 0 auto
							width 200px
							border-radius 3px
							a
								display block
								text-decoration none
								color Verde_Ascent
								font-size 1.2em
								padding .5em
								cursor pointer
			&__right
				width 100%
				&--image
					background-image url('../images/right.png')
					width 60%
					background-size contain
					margin 0 auto
					background-position center
					background-repeat no-repeat
					height 200px
		.LandingSecondary
			padding-bottom 3em
			&__right
				margin 0 auto
				// height 300px
				background-color white
				&--image
					width 100%
					height 250px
					background-repeat no-repeat
					background-size contain
					background-position center
					// position relative
					// padding 10%
					background-image url('../images/cover_window.png')
					.slidesjs-control, .slidesjs-container
						width 100% !important
						min-width 320px !important
						max-width 400px !important
						height 250px !important
						margin 0 auto
					.cover_window1
