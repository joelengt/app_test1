			&__left
				max-width 400px
				margin 0 auto
				&--content
					.SecondaryLeft_Text
						padding 1em
						h3
							font-size 2em
							font-weight lighter
							color gris_E
						p
							color gris_D
							font-size 18px
							font-weight 300
					.SecondaryLeft_TypeOption
						display flex
						flex-wrap wrap
						justify-content space-around
						&__tools
							width 150px
							cursor pointer
							margin-bottom 1em
							text-align center
							&--image
								display inline-block
								background-color verde_B
								width 100px
								text-align center
								img
									width 90%
									margin 10% auto
							&--text
								font-weight lighter
								color gris_D
						&__soundBook
							width 150px
							cursor pointer
							margin-bottom 1em
							text-align center
							&--image
								display inline-block
